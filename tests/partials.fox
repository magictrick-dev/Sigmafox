begin;
    variable foo 4;
    variable bar 4;
    foo := 10;
    bar := 20;
    variable baz 4 := foo + bar;

    scope;
        variable hello 4 := 10;
        variable world 4 := foo;
        variable foo 4 := 10;
    endscope;

    while foo < 10;
        variable hello 4 := 10;
        variable world 4 := foo;
        variable foo 4 := 10;
    endwhile;

end;
