begin;

    variable primes 4 10;

    primes(1)   :=  2;
    primes(2)   :=  3;
    primes(3)   :=  5;
    primes(4)   :=  7;
    primes(5)   := 11;
    primes(6)   := 13;
    primes(7)   := 17;
    primes(8)   := 19;
    primes(9)   := 23;
    primes(10)  := 29;

    variable primes_sum 4 := 0;
    loop index 1 10;
        primes_sum := primes_sum + primes(index);
    endloop;

end;
